{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Rebus": "Information"
    }
  },
  "ConnectionStrings": {
    "ConsolidadoConnection": "Host=localhost;Port=5434;Database=fluxocaixa-consolidado;Username=saga;Password=saga123"
  },
  "RabbitMQ": {
    "Host": "localhost:5672",
    "Username": "saga",
